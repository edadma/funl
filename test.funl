val vowel = cset( 'aeiou' )
val text = 'test string'

every write( text ? upto(vowel) )

text ?
  if tab(upto(vowel) + 1)
    write(tab(0))

text ?
  while write(tab(upto(' ')))
    tab(many(' '))

  write(tab(0))

text ?
  while tab(upto(alphanum))
    write(tab(many(alphanum)))

write( "The theory is fallacious" ? match("The") )
